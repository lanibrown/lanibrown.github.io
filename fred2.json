{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
    "url": "https://raw.githubusercontent.com/lanibrown/lanibrown.github.io/main/observations-2.json",
    "format": {"type": "json", "property": "observations"}
  },
      "title": {
    "text": "Hours worked by full-time and part-time employees",
    "subtitle": "Millions of hours, Annual, Not Seasonally Adjusted. Source: FRED",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },

    "width":600,
    "height":350,
  "mark": "line",
  "encoding": {
    "x": {"field": "date", "type": "temporal", "title":"Date"},
    "y": {"field": "value", "type": "quantitative","title":"Hours Worked (Millions)"}
  }
}
