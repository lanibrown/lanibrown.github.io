{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
      "text": "People Working From Home",
      "subtitle":"The percentage of people reported in each working from home status. Source: ONS, April 2020",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"},
  "description": "Stock prices of 5 Tech Companies over Time.",
  "data": {"url": "https://raw.githubusercontent.com/lanibrown/lanibrown.github.io/main/Homeworking.csv"},
  "transform": [{"filter": "datum.Category!=='IBM'"}],
  "height": 400,
  "width": 300,

  "mark": "line",
  "encoding": {
    "x": {"field": "Year", "type": "temporal", "title": "Year"},
    "y": {"field": "WorkingFromHome", "type": "quantitative",
    "title": "%", "scale": {"domain": [20, 46]}},
    "color": {"field": "Category", "type": "nominal"}
  }
}
